import java.util.Arrays;
public class matrix {
	public static void main(String[] args) {
		int time = Integer.parseInt(args[0]);
		int head = 0;
		int tail = 0;
		int flip = 0;
		while(flip<time) {
			double value = Math.random();
			if (value <= 0.5) {
				head += 1;
				System.out.println("head count" + head);
				flip++;
			}else {
				tail += 1;
				System.out.println("Tail count" + tail);
				flip ++;
			}
		}
	}
}
