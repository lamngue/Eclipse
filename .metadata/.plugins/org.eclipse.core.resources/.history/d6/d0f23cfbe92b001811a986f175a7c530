public class Checkerboard {
    public static void main(String[] args) {
    	System.out.println("12 midnight");
    	for (int i = 0;i<23;i++ ) {
    		System.out.println(i + " am");
    		if (i==12) {
    			System.out.println(i + " noon");
    			i = 1
    		}
    		else if (i >12) {
    			System.out.println(i + " pm");
    		}
    	}
    	
    }
}
